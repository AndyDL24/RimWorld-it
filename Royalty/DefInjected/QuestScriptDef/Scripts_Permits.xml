<LanguageData>
  
  <!-- EN:
    <li>questName->Laborers</li>
    <li>questDescription-></li>
  -->
  <Permit_CallLaborers.questDescriptionAndNameRules.rulesStrings>
    <li>questName->Lavoratori</li>
